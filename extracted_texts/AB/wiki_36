func MergeSort(list []int) []int {
 var length = len(list)
 if length &lt; 2 {
 return list
 var mid = length / 2
 return merge(MergeSort(list[:mid]), MergeSort(list[mid:]))
func merge(x, y []int) []int {
 var r []int = make([]int, len(x)+len(y))
 for i, j := 0, 0; ; {
 if i &lt; len(x) &amp;&amp; (j == len(y) || x[i] &lt; y[j]) {
 r[i+j] = x[i]
 i++
 } else if j &lt; len(y) {
 r[i+j] = y[j]
 j++
 } else {
 break
 return r
