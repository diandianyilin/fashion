 g_sort(L, length(L)).
g_sort([_, _ | _] = L, Length) -&gt;
 SplitNum = trunc(Length / 2),
 {L1, L2} = lists:split(SplitNum, L),
 g_merge(g_sort(L1, SplitNum), g_sort(L2, Length - SplitNum));
