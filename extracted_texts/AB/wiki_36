def merge list
 return list if list.size &lt; 2
 pivot = list.size / 2
 # Merge
 lambda { |left, right|
 final = []
 until left.empty? or right.empty?
 final « if left.first &lt; right.first; left.shift else right.shift end
 end
 final + left + right
 }.call merge(list[0...pivot]), merge(list[pivot..-1])
end
