 lSize := len(left)
 if lSize &gt;= 2 {
 left = merge(left)
 right := data[sum/2:]
 rSize := len(right)
 if rSize &gt;= 2 {
 right = merge(right)
 j := 0
 t := 0
 arr := make([]int, sum)
