C++.
auto eratosthenes(int upperbound) {
 std::vector&lt;bool&gt; flag(upperbound + 1, true);
 flag[0] = flag[1] = false; //exclude 0 and 1
 for (int i = 2; i * i &lt;= upperbound; ++i) {
 if (flag[i]) {
 for (int j = i * i; j &lt;= upperbound; j += i)
 flag[j] = false;
 } 
 return flag;
