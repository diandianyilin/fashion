 is_prime[i] = true;
 is_prime[0] = is_prime[1] = 0;
 for (int i = 2; i &lt;= n; i++) {
 if (is_prime[i]) {
 prime[p++] = i;
 if (1ll * i * i &lt;= n) {
 for (int j = i * i; j &lt;= n; j += i) {
 is_prime[j] = 0;
 return p;
