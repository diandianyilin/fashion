 addEdge(v2, v3, 8);
 addEdge(v2, v5, 4);
 addEdge(v2, v4, 5);
 addEdge(v3, v4, 3);
 addEdge(v3, v5, 9);
 addEdge(v5, v4, 7);
 addEdge(v5, v1, 2);
 addEdge(v4, v2, 2);
