public class Prim {
 public static List&lt;Vertex&gt; vertexList = new ArrayList&lt;Vertex&gt;();//结点集
 public static List&lt;Edge&gt; EdgeQueue = new ArrayList&lt;Edge&gt;();//边集
 public static List&lt;Vertex&gt; newVertex = new ArrayList&lt;Vertex&gt;();//已经 访问过的结点
